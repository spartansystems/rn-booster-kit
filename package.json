{
  "name": "RNBooster",
  "version": "0.0.1",
  "description": "React Native Booster Kit",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "./node_modules/.bin/eslint '*.js' 'app/**'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spartansystems/rn-booster-kit.git"
  },
  "keywords": [
    "react",
    "react-native",
    "booster",
    "starter",
    "mobile"
  ],
  "author": "Spartan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spartansystems/rn-booster-kit/issues"
  },
  "homepage": "https://github.com/spartansystems/rn-booster-kit#readme",
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "Promise": true
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": true
        }
      ],
      "max-len": [
        "error",
        80,
        2,
        {
          "ignoreUrls": true
        }
      ]
    },
    "plugins": [
      "react"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-preset-react-native": "^1.9.0",
    "eslint": "^2.13.0",
    "eslint-plugin-react": "^5.2.1"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-native": "^0.27.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "remote-redux-devtools": "^0.3.3",
    "reselect": "^2.5.1"
  }
}
